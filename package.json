  "main": "angular2-cloudtasks.js",
  "typings": "./angular2-cloudtasks.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Cloudtasks/angular2-cloudtasks.git"
  },
  "keywords": [
    "angular2",
    "AngularJS",
    "angular2-cloudtasks",
    "cloudtasks",
    "image",
    "images",
    "processing",
